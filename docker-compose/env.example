# Пример файла переменных окружения для docker-compose.yml
# Скопируйте этот файл в .env и измените значения
# Команда: cp env.example .env

# Redis
REDIS_PASSWORD=your-secure-redis-password-here

# Supabase
SUPABASE_DB_PASSWORD=your-secure-supabase-password-here
SUPABASE_ORG_NAME=My Organization
SUPABASE_PROJECT_NAME=My Project

# n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-n8n-password-here
N8N_WEBHOOK_URL=http://localhost:5678/
N8N_METRICS=true
N8N_LOG_LEVEL=info
N8N_WORKERS_COUNT=2
