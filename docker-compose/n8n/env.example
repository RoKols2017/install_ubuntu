# Пример файла переменных окружения для n8n
# Скопируйте этот файл в .env и измените значения
# Команда: cp env.example .env

# Базовые настройки аутентификации
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-password-here
N8N_ENCRYPTION_KEY=your-secure-n8n-encryption-key-here
N8N_USER_MANAGEMENT_JWT_SECRET=your-secure-n8n-jwt-secret-here

# База данных PostgreSQL (Supabase)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=pgbouncer
DB_POSTGRESDB_PORT=6432
DB_POSTGRESDB_DATABASE=postgres
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=your-supabase-password-here

# Redis для очередей
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_PASSWORD=your-redis-password-here

# Настройки воркера
N8N_WORKERS_ENABLED=true
N8N_WORKERS_COUNT=2

# Webhook URL (измените на ваш домен для production)
WEBHOOK_URL=http://localhost:5678/

# Дополнительные настройки
N8N_METRICS=true
N8N_LOG_LEVEL=info

# Настройки времени выполнения (опционально)
N8N_EXECUTION_TIMEOUT=3600
N8N_EXECUTION_TIMEOUT_MAX=7200

